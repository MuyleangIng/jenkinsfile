pipeline {
    agent any;
    parameters{
        choice(name: "TEST_CHOICE", choices: ["production", "staging", "development"], description: "Sample multi-choice parameter")
    }
    stages {
        stage('test choice'){
            steps{
                echo "${TEST_CHOICE}"
                script{
                    def gitbranch = "${TEST_CHOICE}"
                    echo "brahc git :" gitbranch 
                }
            }

        }
        
    }
   
}